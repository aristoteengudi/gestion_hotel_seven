
<table id="datatable-buttons" class="table table-striped table-bordered dt-responsive nowrap" style="border-collapse: collapse; border-spacing: 0; width: 100%;">
    <thead>
    <tr>
        <th>#chambre_id</th>
        <th>numéro_chambre</th>
        <th>Etat Disponibilité</th>
        <th>Catégorie</th>
        <th>Coût</th>
        <th></th>
    </tr>
    </thead>

    <tbody>
    {% if filter_data|length > 0 %}
        {% for value in filter_data %}
            <tr>
                <td>{{ value.id }}</td>
                <td>{{ value.numero_chambre }}</td>
                <td>{{ value.etat_disponibilite }}</td>
                <td>{{ value.categorie }}</td>
                <td>{{ value.cout }}</td>
                <td></td>
            </tr>

        {% endfor %}
    {% else %}
        {% for value in list_chambre %}
            <tr>
                <td>{{ value.id }}</td>
                <td>{{ value.numero_chambre }}</td>
                <td>{{ value.etat_disponibilite }}</td>
                <td>{{ value.categorie }}</td>
                <td>{{ value.cout }}</td>
                <td></td>
            </tr>
        {% endfor %}

    {% endif %}

    </tbody>
</table>


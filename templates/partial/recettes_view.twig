                <tbody>
                {% for value in income_expense %}
                    <tr>
                        <td>{{ value._date }}</td>
                        <td>{{ value.incomes }} $</td>
                        <td>{{ value.expenses }} $</td>
                        <td>{{ value.profit }} $</td>
                    </tr>
                {% endfor %}
                </tbody>
            </table>
    {% endif %}
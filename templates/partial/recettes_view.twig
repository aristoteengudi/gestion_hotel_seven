    {% if filter_data|length > 0 %}
        <table id="datatable-buttons" class="table table-striped table-bordered dt-responsive nowrap" style="border-collapse: collapse; border-spacing: 0; width: 100%;">
            <thead>
            <tr>
                <th>#id</th>
                <th>numéro_chambre</th>
                <th>cout</th>
                <th>currency</th>
                <th>telephone</th>
                <th>nombre_occupant</th>
                <th>status_reservation</th>
                <th>date_debut_reservation</th>
                <th>date_fin_reservation</th>
                <th>date_creation_reservation</th>
                <th>date_maj</th>
            </tr>
            </thead>

            <tbody>
            {% for value in reservations %}
                <tr>
                    <td></td>
                    <td>{{ value.numero_chambre }}</td>
                    <td>{{ value.cout }}</td>
                    <td>{{ value.chambre_currency }}</td>
                    <td>{{ value.telephone }}</td>
                    <td>{{ value.nombre_personne }}</td>
                    <td>{{ value.status_reservation }}</td>
                    <td>{{ value.stardate }}</td>
                    <td>{{ value.endate }}</td>
                    <td>{{ value.created_at }}</td>
                    <td>{{ value.updated_at }}</td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    {% else %}
        <table id="datatable-buttons" class="table table-striped table-bordered dt-responsive nowrap" style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                <thead>
                <tr>
                    <th>Date (Mois)</th>
                    <th>Balance</th>
                    <th>Balance Negative (Dépense)</th>
                    <th>Balance Restante</th>
                </tr>
                </thead>

                <tbody>
                {% for value in reservations %}
                    <tr>
                        <td>{{ value.prenom }} {{ value.noms }}</td>
                        <td>{{ value.numero_chambre }}</td>
                        <td>{{ value.cout }}</td>
                        <td>{{ value.chambre_currency }}</td>
                    </tr>
                {% endfor %}
                </tbody>
            </table>
    {% endif %}
